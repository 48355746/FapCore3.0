@model Fap.AspNetCore.ViewModel.JqGridViewModel
<fap-grid id="assess-objectives" grid-model="Model" wrapper="modal-body" register-authority="false" view-records="true"  multi-select="true" oper-delete="true" shrink-fit="false"></fap-grid>

<script>
    function refreshAssessObjectives() {
        refreshGrid("grid-assess-objectives");
    }
    function getObjectives() {
        return getSelectedRows("grid-assess-objectives");
    }
</script>