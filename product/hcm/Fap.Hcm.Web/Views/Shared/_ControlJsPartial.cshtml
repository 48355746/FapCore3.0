@if (_applicationContext.Language == Fap.Core.MultiLanguage.MultiLanguageEnum.En)
{
    //多语
    <script src="~/Content/JqGrid/i18n/grid.locale-en.js" asp-append-version="true"></script>
    <script src="~/Content/fileinput/locales/LANG.js" asp-append-version="true"></script>
}
else if (_applicationContext.Language == Fap.Core.MultiLanguage.MultiLanguageEnum.ZhCn)
{
    <script src="~/Content/JqGrid/i18n/grid.locale-cn.js" asp-append-version="true"></script>
    <script src="~/Content/fileinput/locales/zh.js" asp-append-version="true"></script>
}
else if (_applicationContext.Language == Fap.Core.MultiLanguage.MultiLanguageEnum.ZhTW)
{
    <script src="~/Content/JqGrid/i18n/grid.locale-tw.js" asp-append-version="true"></script>
    <script src="~/Content/fileinput/locales/zh-TW.js" asp-append-version="true"></script>
}
else if (_applicationContext.Language == Fap.Core.MultiLanguage.MultiLanguageEnum.Ja)
